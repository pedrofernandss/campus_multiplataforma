{
  "expo": {
    "name": "Campus",
    "newArchEnabled": true,
    "slug": "campus_multiplataforma",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/campus-icon.png",
    "scheme": "campus",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/campusplash.png",
      "backgroundColor": "#6C0318"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Este aplicativo precisa acessar sua galeria para permitir que você selecione vídeos."
      }
    },
    "android": {
      "icon": "./assets/icons/campus-icon.png",
      "package": "com.pedrofernandss.campus_multiplataforma",
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/campus-icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "429b6580-bf80-4611-a0b7-96403274a61a"
      }
    }
  }
}
